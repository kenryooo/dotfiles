[color]
	ui = true
[user]
	name = kenryooo
	email = kenryo.ominami@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	autocrlf = input
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
	helper = osxkeychain
[ghq]
	root = ~/develop/git
	root = ~/go/src
	root = ~/develop/IdeaProjects
	root = ~/develop/SourceTree
; [include]
; 	path = ~/.gitconfig.local
